<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Hours_Must_Be_Positive</fullName>
    <active>true</active>
    <errorConditionFormula>Hours__c &lt;= 0</errorConditionFormula>
    <errorDisplayField>Hours__c</errorDisplayField>
    <errorMessage>Hours must be greater than zero.</errorMessage>
</ValidationRule>
